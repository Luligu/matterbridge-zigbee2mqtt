            "enum": ["all_members_off", "last_member_state"],
            "requiresRestart": true,
            "title": "Group off state",
            "type": ["string"]
          },
          "optimistic": {
            "type": "boolean"
          },
          "qos": {
            "type": "number"
          },
          "retain": {
            "type": "boolean"
          }
